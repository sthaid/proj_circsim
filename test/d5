clear_all

add power      c3  d3 10v    
add wire       c3  c4 
add resistor   c4  d4 5  
add resistor   d4  e4 10
add wire       e4  e3
add wire       e3  d3
add diode      d4  d5
add resistor   d5  e5 8.59
add wire       e5  e4

ground d3

set center d4,-4,-19
set scale  322

set scope_span_t 1ms
set scope_a voltage,0,5,d4
set scope_b voltage,0,5,d5

step


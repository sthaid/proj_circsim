clear_schematic

add power      c2  d2 200,100hz
add wire       c2 c3
add diode      c3 c4
add diode      d3 c3
add diode      d3 d4
add diode      d4 c4
add resistor   c4 c5 10
#add wire       c4 c5 
add wire       c5 c6
add wire       d6 d5
add wire       d5 e5
add wire       e5 e3
add wire       e3 d3
add wire       d2 d4 remote,green

add            resistor c6 d6  50
add            capacitor c5 d5  1mf

ground d2

set center d4,-4,-19
set scale  266

set scope_t 20ms   
set scope_a voltage,-500,500,c6,d6  RESISTOR
set scope_b current,-2,2,c6,d6  RESISTOR
set scope_c voltage,-2,2,d5  RETURN
set scope_d voltage,-200,200,c2  POWER

#set scope_mode continuous

set delta_t 100ps


clear_schematic

add power      b1   c1   5.00V
add wire       b1   b2   
add resistor   b2   c2   500
add capacitor  c2   d2   500uF
add wire       c2   c3   
add wire       d2   d3   
add capacitor  c3   d3   500uF
add resistor   d2   d1   500
add wire       d1   c1   

ground d1

set center  c2,-51,-27
set scale   322

set scope_t 1s
set scope_a  voltage,0,5,b2,c2
set scope_b  voltage,0,5,c2,d2
set scope_c  voltage,0,5,d2,d1

model run 1

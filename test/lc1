#                               1
# resonant frequency =  -------------------
#                        2 * PI * sqrt(L*C)
#
# f = 1  L = .159 C = .159

clear_schematic

add capacitor  c4   d4   .1591549F
add inductor   c5   d5   .1591549H,1A
add wire       c4   c5   
add wire       d4   d5   

ground d4

set center  d5,94,143
set scale   354

set scope_t 5s
set scope_a voltage,-1,1,c4
set scope_b current,-1,1,c5,d5

model run 5s

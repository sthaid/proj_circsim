# XXX  NOT WORKING with standard delta_t,  does work with 1ns
# set delta_t 1ns

clear_schematic

add power      b1  c1 5v,5hz
add diode      b1  b2 
add resistor   b2  c2 100
add diode      c2  c1

ground c1

set center c2,19,100
set scale  322

set scope_t 1
set scope_a voltage,-5,5,b1,c1,POWER
set scope_b voltage,-5,5,b1,b2,DIODE
set scope_c voltage,-5,5,b2,c2,RESISTOR
set scope_d voltage,-5,5,c2,c1,DIODE

model run .2


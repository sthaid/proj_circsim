clear_schematic

add power      b1  c1 5v
add wire       b1  b2 
add resistor   b2  c2 100
add diode      c2  d2 
add wire       d2  d1
add wire       d1  c1

ground c1

set center c1,-151,-29
set scale  322

set scope_t 0.50ms
set scope_a voltage,0,5,b2,c2
set scope_b voltage,0,5,c2,d2

model run 1ms


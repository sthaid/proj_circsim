# time_constant = L / R

clear_schematic

add power      b1   c1   5.00V
add wire       b1   b2   
add resistor   b2   c2   10
add inductor   c2   d2   10H
add wire       d2   d1   
add wire       d1   c1   

ground c1

set stop_t       1s
set delta_t      1ns
set dcpwr_ramp_t 1ms
set grid         off
set current      on
set voltage      on
set component    value
set center       c2,-29,-25
set scale        293


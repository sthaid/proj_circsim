clear_schematic

add power      b1   c1   5.00V
add wire       b1   b2   
add resistor   b2   c2   5 
add inductor   c2   d2   20H
add inductor   c3   d3   20H
add wire       c2   c3
add wire       d2   d3
add resistor   d2   d1   5
add wire       d1   c1   

ground c1

set center       c2,-29,-25
set scale        293

set scope_t 1
set scope_a voltage,0,5,c2,d2

model run 1
